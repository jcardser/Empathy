@model Empathy.Data.Entities.History

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles de procedimiento</h1>

<div>
    <h4>Historia Clinica</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Summary)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Summary)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Symptoms)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Symptoms)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Notes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Notes)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BloodPressure)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BloodPressure)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HeartRate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HeartRate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BreathingFrequency)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BreathingFrequency)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Temperature)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Temperature)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Diagnosis)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Diagnosis)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PhysicalExam)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PhysicalExam)
        </dd>
        <dt class="col-sm-2">
            Procedimientos
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HistoryProcedures.FirstOrDefault().Procedure.TypeProcedure)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-outline-primary">Editar</a> |
    <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
</div>
