@model Empathy.Data.Entities.Procedure

@{
    ViewData["Title"] = "Create";
}

<h1>Solicitar Cita Médica</h1>

<h4>Cita Médica</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TypeProcedure" class="control-label"></label>
                <input asp-for="TypeProcedure" class="form-control" />
                <span asp-validation-for="TypeProcedure" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
</div>

